<section>
<h2>Welcome to user index page</h2>
<ul>
  <% @users.each do |user| %>
    <li>
      <img src='<%= user.photo %>' alt="<%= user.name %>'s profile picture" />
      <div>
        <h2><%= link_to user.name, user_path(user) %></h2>
        <p>Number of Posts: <%= user.posts_counter %></p>
      </div>
    </li>
  <% end %>
</ul>
</section>